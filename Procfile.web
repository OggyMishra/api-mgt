dnsmasq: dnsmasq -p $DNSMASQ_PORT -N -o --listen-address=127.0.0.1 --keep-in-foreground
nginx: nginx -p $NGINX_WORKING_DIR -c $NGINX_CONFIG -g "daemon off;"
serf: serf agent -bind $SERF_CLUSTER_LISTEN -rpc-addr $SERF_CLUSTER_LISTEN_RPC -encrypt $SERF_ENCRYPT -profile wan -node $SERF_NODE_NAME -event-handler $SERF_EVENT_HANDLER -event-handler=member-join,member-leave,member-failed,member-update,member-reap,user:kong=/app/nginx-working/serf_event.sh -log-level=err